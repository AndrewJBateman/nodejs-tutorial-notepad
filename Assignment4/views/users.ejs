<%- include('includes/head.ejs') %>

<body>
	<%- include('includes/navigation.ejs') %>

	<main>
		<% if (users.length > 0) { %>
		<p>Total users: <%= users.length %></p>
		<ul>
			<% for (const user of users) { %>
			<li><%= user.name%></li>
			<% } %>
		</ul>

		<% } else { %>
		<h1>No users found</h1>
		<% } %>
	</main>
	<%- include('includes/end.ejs') %>
</body>
